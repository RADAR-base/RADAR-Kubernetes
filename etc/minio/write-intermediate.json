{
  "Version": "2012-10-17",
  "Statement": [
	      {
      "Action": ["s3:ListBucket"],
      "Effect": "Allow",
      "Resource": ["arn:aws:s3:::radar-intermediate-storage"]
    },
    {
      "Action": [
        "s3:GetBucketLocation",
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::radar-intermediate-storage/*"
      ],
      "Sid": ""
    }
  ]
}
